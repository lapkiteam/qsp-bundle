{
  "name": "@fering-org/qsp-bundle",
  "version": "0.1.0",
  "scripts": {
    "clean": "gulp clean",
    "build": "gulp build",
    "dev": "npm run build && cd build && node bin/qsp-bundle.js",
    "deploy": "gulp deploy",
    "globalInstall": "cd build && npm install -g .",
    "globalUninstall": "npm uninstall -g @fering-org/qsp-bundle",
    "test": "gulp test",
    "test:watch": "gulp testWatch",
    "release": "standard-version",
    "pack": "npm run deploy && cd build && npm pack",
    "publish": "npm run deploy && cd build && npm publish --access public"
  },
  "devDependencies": {
    "@types/gulp": "^4.0.17",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "gulp": "^5.0.1",
    "gulp-json-modifier": "^1.0.0",
    "jest": "^29.7.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.4.0",
    "typescript": "~4.7.3"
  },
  "dependencies": {
    "@fering-org/functional-helper": "^0.6.2",
    "chokidar": "^4.0.3",
    "commander": "^13.1.0",
    "immutability-helper": "^3.1.1",
    "ts-functional-pipe": "^3.1.2"
  }
}
